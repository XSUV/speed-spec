{
  "$schema": "https://raw.githubusercontent.com/DavidAnson/markdownlint/main/schema/markdownlint-config-schema.json",
  "default": true,
  "first-line-h1": false,
  "fenced-code-language": false,
  "ul-style": {
    "style": "dash"
  },
  "no-trailing-spaces": {
    "br_spaces": 0,
    "list_item_empty_lines": false
  },
  "list-marker-space": false,
  "line-length": false,
  "no-inline-html": false,
  "no-duplicate-header": false,
  "proper-names": {
    "names": [
      "JavaScript",
      "TypeScript",
      "HTML",
      "CSS",
      "AJAX",
      "JSON",
      "DOM",
      "BOM",
      "WebSocket",
      "ECMAScript",
      "ECMAScript 2015",
      "ECMAScript 6",
      "ES6",
      "ES2015",
      "ESNext",
      "jQuery",
      "React",
      "React Native",
      "Bootstrap",
      "Gulp",
      "Grunt",
      "webpack",
      "npm",
      "pnpm",
      "yarn",
      "Node.js",
      "NodeJS",
      "MySQL",
      "MongoDB",
      "Redis",
      "Apache",
      "MIT",
      "GitHub",
      "GitLab",
      "GitCafe",
      "Chrome",
      "Firefox",
      "Safari",
      "Internet Explore",
      "IE",
      "Opera",
      "Android",
      "iOS",
      "Windows",
      "Ubuntu",
      "Linux",
      "H5",
      "Visual Studio Code"
    ],
    "code_blocks": false
  }
}
